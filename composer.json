{
    "name": "contentreactor/shipping-time",
    "description": "Calculation of shipping time during holidays and non-holiday periods.",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "support@contentreactor.com"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.8.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "contentreactor\\shipping\\time\\": "src/"
        }
    },
    "extra": {
        "handle": "contentreactor-shipping-time",
        "name": "Content Reactor Shipping Time",
        "developer": "Content Reactor",
        "documentationUrl": ""
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

